<h1 class="pb-4">Explore Newest</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  <% if(media != '') { %>
    <% media.forEach(function(media, index){ %>
    <a href="/media/<%= media._id %>" class="col text-center category__link">
      <div class="category__img category__img--large shadow">
        <img src="/uploads/<%= media.image %>" alt="<%= media.name %>" loading="lazy">
      </div>
      <div class="pt-1"><%= media.name %></div>
    </a>
    <% }) %>
  <% } else { %>
    <p>No items found.</p>
  <% } %> 
</div> 